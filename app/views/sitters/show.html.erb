<h1>Welcome
	<%= @sitter.first %>
	<%= @sitter.last %></h1>
<%= image_tag @sitter.avatar.url(:medium) %>
<p><%= @sitter.description %></p>
<%= link_to "edit sitter", edit_sitter_registration_path(@sitter) %>
<br>
<hr>
<h3>dogs I am watching</h3>
<% @sitter.rentals.each do |rental| %>
<% if rental.approved? && rental.done_renting == false %>
 <%= rental.dog.name %> <%= link_to "finish rental", rental_path(rental), method: :patch %>
 <br>
 <% end %>
 <% end %>
 <hr>
 <h3>dogs I have watched in the past</h3>
 <% @sitter.rentals.each do |rental| %>
 <% if rental.done_renting? %>
  <%= rental.dog.name %>
  <br>
	<% end %>
	<% end %>
